	<link rel="icon" href="favicon.ico">
	<title>{{ dictionary.title }} | {{app_info.name}}</title>
</head>
<body>
  {% import 'macros.twig' as macro_main %}
  {% import 'macro-item.twig' as macro_item %}
  <!-- header -->
  	{{macro_main.all_header(login,dictionary,user,app_category,theme_category)}}
  <!-- /header -->


	<!-- SECTION HEADLINE -->
	<div class="section-headline-wrap">
		<div class="section-headline" id="addons-buy">
			<h2>{{app_info.name}}</h2>
			<input id="id-item" style="display:none" value="{{app_info.id}}"></input>
			<input id="item-is-app" style="display:none" value="{{info_page.app}}"></input>
			<!--p>Home<span class="separator">/</span>Web Design<span class="separator">/</span><span class="current-section">HTML Templates</span></p-->
		</div>
	</div>
	<!-- /SECTION HEADLINE -->

	<!-- SECTION -->
	<div class="section-wrap">
		<div class="section">
			<!-- SIDEBAR -->
			<div class="sidebar right">


				<!-- SIDEBAR ITEM -->
				<div class="sidebar-item">
					{% if info_page.app == 0 %}
					<h6 style="text-align: center;"> Link Doc </h6>
					{% else %}
					<h6 style="text-align: center;">Web site</h6>
					{% endif %}
					<hr class="line-separator">
					<a href="{{app_info.website}}"><h6 style="text-align: center;">{{app_info.website}}</h6></a>
					<div class="clearfix"></div>
				</div>
				<!-- /SIDEBAR ITEM -->

        <!-- SIDEBAR ITEM -->
        {% if info_page.app == 0 %}
				<div class="sidebar-item" id="template">
          {% for i in array_images %}
					<a href="{{info_footer.path_file}}#template{{i.id}}" id="{{i.id}}" for="{{i.id}}" class="button mid dark spaced"><span class="primary">Templete {{i.id}}</span></a>
          {% endfor %}
					<div class="clearfix"></div>
				</div>
        {% endif %}
				<!-- /SIDEBAR ITEM -->

        <!-- SIDEBAR ITEM -->
				<div class="sidebar-item" id="item-page">
					<p class="price large" id="coin"><span id="coin">{{dictionary.coin}}$</span></p>
					<hr class="line-separator">
					<form id="aux_form" name="aux_form"></form>
					{% for plan in plans %}
					<!-- CHECKBOX -->
					<input type="radio" id="{{plan.id}}" name="{{plan.id}}" form="aux_form" {{plan.checked}} class="checkbox-plan">
					<label class="b-label linked-check" for="{{plan.id}}">
						<span class="checkbox primary"><span></span></span>
						{{plan.name}}
						<input id="i-{{plan.id}}" style="display:none" value="{{plan.value}}"></input>
						<input id="s-{{plan.id}}" style="display:none" value="{{plan.id}}"></input>
					</label>
					<!-- /CHECKBOX -->
					<p class="license-text" data-license="{{plan.id}}" >{{plan.desc}}</p>
					{% endfor %}
					{# not implemented
					<!-- CHECKBOX -->
					<input type="radio" id="regular-license" name="regular-license" form="aux_form" checked>
					<label class="b-label linked-check" for="regular-license">
