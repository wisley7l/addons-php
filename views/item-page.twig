		        <input type="hidden" id="addons-app-is_app" name="is_app" />
						<input type="hidden" id="addons-app-id_plan" name="id_plan" />
						<input type="hidden" id="addons-app-value" name="value" />
						<input type="hidden" id="addons-app-id_template" name="id_template" />

