					<!-- POST IMAGE -->
					{{macro_item.image_main(image_main,info_page)}}
					<!-- /POST IMAGE -->
					{% else %}
						{% for i in array_images %}
							{% if loop.index0 > 0 %}
							<div id="image-{{i.id}}" style="display:none;">
								{{macro_item.image_main(i,info_page)}}
							</div>
							{% else %}
								<div id="image-{{i.id}}" style="display: block;">
									{{macro_item.image_main(i,info_page)}}
								</div>
							{% endif %}
						{% endfor %}
					{% endif %}
					{# themes not outher images #}
					{% if info_page.app == 1 %}

					<!-- POST IMAGE SLIDES -->
					<div class="post-image-slides">
						<!-- IMAGE SLIDES WRAP -->
						{{macro_item.outher_images_item(image_main,array_images)}}
						<!-- IMAGE SLIDES WRAP -->
					</div>
					<!-- /POST IMAGE SLIDES -->
					{% endif %}


					{% if app_info.video == '' %}

