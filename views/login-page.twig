			<div class="form-popup-content-links">
				<a href="#" class="button mid dark">Submit your <span class="primary">Entry!</span></a>
				<div class="form-popup-content-links-social">
					<a href="#" class="button mid wfb"></a>
					<a href="#" class="button mid wtwt"></a>
				</div>
			</div>
		</div>
		<!-- /FORM POPUP CONTENT -->
	</div>
	<!-- /FORM POPUP -->


	<!-- SECTION -->
	<div class="section-wrap">
		<div class="section">
			<!-- FORM BOX ITEM -->
			<div class="form-box-item">
				<h4>Alert &amp; Notification Boxes Preview</h4>
				<hr class="line-separator">
				<!-- ALERT BOXES PREVIEW -->
				<div class="alert-boxes-preview">
					<div class="alert-boxes-preview-links">
						<a href="#promo-popup" class="button mid dark promo-popup">
							Preview <span class="primary">Big Promo Box (Popup)</span>
						</a>
					</div>
				</div>
				<!-- /ALERT BOXES PREVIEW -->
			</div>
			<!-- /FORM BOX ITEM -->
        </div>
	</div>
	<!-- /SECTION -->

	<div class="shadow-film closed"></div>


<!-- jQuery -->
<script src="../js/vendor/jquery-3.1.0.min.js"></script>
<!-- xmAlert -->
