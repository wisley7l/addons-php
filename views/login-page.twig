<body>

	<!-- FORM POPUP -->
	<div id="promo-popup" class="form-popup promo mfp-hide">
		<!-- CLOSE BTN -->
		<div class="close-btn">
			<!-- SVG PLUS -->
			<svg class="svg-plus">
				<use xlink:href="#svg-plus"></use>
			</svg>
			<!-- /SVG PLUS -->
		</div>
		<!-- /CLOSE BTN -->

		<!-- PROMO BG -->
		<div class="promo-bg"></div>
		<!-- /PROMO BG -->

		<!-- FORM POPUP CONTENT -->
		<div class="form-popup-content">
			<h4 class="popup-title">Enter our Holidays Contest!</h4>
			<p class="info">Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</p>
